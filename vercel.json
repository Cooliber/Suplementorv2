{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/next"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"headers": {
				"Cache-Control": "public, max-age=300, s-maxage=300, stale-while-revalidate=600"
			}
		},
		{
			"src": "/_next/static/(.*)",
			"headers": {
				"Cache-Control": "public, max-age=31536000, immutable"
			}
		},
		{
			"src": "/(.*\\.(glb|gltf|obj|fbx))",
			"headers": {
				"Cache-Control": "public, max-age=86400, s-maxage=86400",
				"Content-Encoding": "gzip"
			}
		},
		{
			"src": "/(.*\\.(png|jpg|jpeg|gif|webp|svg|ico))",
			"headers": {
				"Cache-Control": "public, max-age=86400, s-maxage=86400"
			}
		},
		{
			"src": "/(.*\\.(woff|woff2|ttf|otf|eot))",
			"headers": {
				"Cache-Control": "public, max-age=31536000, immutable"
			}
		},
		{
			"src": "/api/health",
			"headers": {
				"Cache-Control": "no-cache, no-store, must-revalidate",
				"Pragma": "no-cache",
				"Expires": "0"
			}
		},
		{
			"src": "/api/supplements/(.*)",
			"headers": {
				"Cache-Control": "public, max-age=1800, s-maxage=3600, stale-while-revalidate=7200",
				"X-Medical-Data": "supplement-information"
			}
		},
		{
			"src": "/api/brain-regions/(.*)",
			"headers": {
				"Cache-Control": "public, max-age=3600, s-maxage=7200, stale-while-revalidate=14400",
				"X-Medical-Data": "educational-content"
			}
		}
	],
	"functions": {
		"src/api/**/*.ts": {
			"maxDuration": 30,
			"memory": 1024
		},
		"src/api/**/generate.ts": {
			"maxDuration": 60,
			"memory": 2048
		},
		"src/api/**/search.ts": {
			"maxDuration": 15,
			"memory": 512
		},
		"src/api/**/embedding.ts": {
			"maxDuration": 120,
			"memory": 3072
		},
		"src/api/**/export.ts": {
			"maxDuration": 90,
			"memory": 2048
		},
		"src/api/**/supplements.ts": {
			"maxDuration": 25,
			"memory": 1024
		},
		"src/api/**/brain-regions.ts": {
			"maxDuration": 20,
			"memory": 768
		},
		"src/api/**/tracking.ts": {
			"maxDuration": 15,
			"memory": 512
		},
		"src/api/**/auth.ts": {
			"maxDuration": 10,
			"memory": 512
		},
		"src/api/**/health.ts": {
			"maxDuration": 5,
			"memory": 256
		}
	},
	"regions": ["fra1", "ams1", "lhr1", "iad1"],
	"framework": "nextjs",
	"installCommand": "bun install --frozen-lockfile",
	"buildCommand": "bun run build",
	"outputDirectory": ".next",
	"devCommand": "bun run dev",
	"maxLambdaSize": "50mb",
	"includeFiles": ["src/data/**", "public/**"],
	"excludeFiles": [
		"**/*.test.ts",
		"**/*.test.tsx",
		"**/*.spec.ts",
		"**/*.spec.tsx",
		"**/*.stories.ts",
		"**/*.stories.tsx",
		"src/stories/**",
		"playwright-report/**",
		"test-results/**",
		".next/cache/**"
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "Referrer-Policy",
					"value": "strict-origin-when-cross-origin"
				},
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'self'; script-src 'self' 'unsafe-eval' 'unsafe-inline' https://vercel.live https://*.vercel-analytics.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; img-src 'self' data: https: blob:; font-src 'self' data: https://fonts.gstatic.com; connect-src 'self' https: wss: https://vercel.live https://*.vercel-analytics.com; worker-src 'self' blob:; frame-ancestors 'none'; form-action 'self'; base-uri 'self'; object-src 'none';"
				},
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=63072000; includeSubDomains; preload"
				},
				{
					"key": "Permissions-Policy",
					"value": "camera=(), microphone=(), geolocation=(), payment=(), usb=(), bluetooth=(), magnetometer=(), accelerometer=(), gyroscope=(), ambient-light-sensor=(), autoplay=(), encrypted-media=(), fullscreen=(self), picture-in-picture=()"
				},
				{
					"key": "X-Medical-App",
					"value": "suplementor-edu"
				},
				{
					"key": "X-Robots-Tag",
					"value": "index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1"
				}
			]
		},
		{
			"source": "/api/(supplements|brain-regions|tracking)/(.*)",
			"headers": [
				{
					"key": "X-Medical-Data-Protection",
					"value": "gdpr-compliant"
				},
				{
					"key": "X-Data-Classification",
					"value": "medical-educational"
				},
				{
					"key": "Cache-Control",
					"value": "private, no-cache, no-store, must-revalidate"
				}
			]
		},
		{
			"source": "/api/auth/(.*)",
			"headers": [
				{
					"key": "X-Auth-Protection",
					"value": "secure-session"
				},
				{
					"key": "Cache-Control",
					"value": "no-cache, no-store, must-revalidate, private"
				}
			]
		}
	],
	"redirects": [
		{
			"source": "/supplements",
			"destination": "/suplementy",
			"permanent": true
		},
		{
			"source": "/brain",
			"destination": "/wiedza",
			"permanent": true
		},
		{
			"source": "/knowledge",
			"destination": "/wiedza",
			"permanent": true
		},
		{
			"source": "/graph",
			"destination": "/wiedza/graf",
			"permanent": true
		}
	],
	"rewrites": [
		{
			"source": "/api/v1/:path*",
			"destination": "/api/:path*"
		},
		{
			"source": "/sitemap.xml",
			"destination": "/api/sitemap"
		}
	],
	"crons": [
		{
			"path": "/api/cron/daily-update",
			"schedule": "0 2 * * *"
		},
		{
			"path": "/api/cron/weekly-report",
			"schedule": "0 3 * * 0"
		},
		{
			"path": "/api/cron/security-scan",
			"schedule": "0 4 * * *"
		},
		{
			"path": "/api/cron/performance-monitor",
			"schedule": "0 5 * * *"
		}
	],
	"github": {
		"enabled": true,
		"autoAlias": true,
		"silent": false
	},
	"scope": "suplementor-team",
	"build": {
		"env": {
			"NEXT_TELEMETRY_DISABLED": "1",
			"GDPR_COMPLIANCE_MODE": "strict",
			"MEDICAL_DATA_PROTECTION": "enabled",
			"POLISH_LOCALIZATION": "enabled",
			"BRAIN_VISUALIZATION_OPTIMIZATION": "enabled"
		}
	},
	"env": {
		"NODE_ENV": "production",
		"VERCEL_ENV": "production",
		"NEXT_PUBLIC_MEDICAL_DISCLAIMER": "enabled",
		"NEXT_PUBLIC_GDPR_COMPLIANCE": "strict",
		"MEDICAL_DATA_PROTECTION": "enabled",
		"GDPR_COMPLIANCE_MODE": "strict",
		"POLISH_LOCALIZATION": "enabled",
		"BRAIN_VISUALIZATION_OPTIMIZATION": "enabled",
		"CDN_OPTIMIZATION": "enabled",
		"REGIONAL_CDN_EUROPE": "enabled",
		"ERROR_TRACKING": "enabled",
		"VERCEL_ANALYTICS": "enabled",
		"FEATURE_BRAIN_3D": "enabled",
		"FEATURE_SUPPLEMENT_TRACKING": "enabled",
		"FEATURE_GAMIFICATION": "enabled",
		"DEPLOYMENT_REGION": "fra1",
		"NEXT_PUBLIC_DEFAULT_LOCALE": "pl",
		"NEXT_PUBLIC_APP_VERSION": "1.0.0",
		"NEXT_PUBLIC_ENABLE_DEBUG": "false",
		"NEXT_PUBLIC_ENABLE_MOCK_DATA": "false"
	},
	"polishLocalization": {
		"enabled": true,
		"primaryLocale": "pl",
		"supportedLocales": ["pl", "en"],
		"fontOptimization": true,
		"regionalCDN": true
	},
	"monitoring": {
		"analytics": {
			"provider": "vercel",
			"gdprCompliant": true,
			"polishLocalized": true
		},
		"performance": {
			"enabled": true,
			"coreWebVitals": true,
			"customMetrics": [
				"brain-visualization-load-time",
				"supplement-search-time",
				"medical-data-response-time"
			]
		},
		"errorTracking": {
			"enabled": true,
			"sampleRate": 0.1,
			"gdprCompliant": true
		}
	},
	"cdn": {
		"optimization": true,
		"regional": {
			"europe": ["fra1", "ams1", "lhr1"],
			"poland": ["fra1"],
			"fallback": ["iad1"]
		},
		"cacheStrategy": "aggressive"
	}
}
