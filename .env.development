# Development Environment Configuration
# Copy this file to .env.local and fill in your actual values

# NextAuth.js Configuration
NEXTAUTH_SECRET="development-secret-key-change-in-production"
NEXTAUTH_URL="http://localhost:3000"

# OAuth Providers (optional - leave empty for development)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""

# Database Configuration (optional for development)
DATABASE_URL="file:./dev.db"
MONGODB_URI="mongodb://localhost:27017/suplementor-dev"

# Medical App Configuration
MEDICAL_DATA_PROTECTION="enabled"
GDPR_COMPLIANCE_MODE="strict"
POLISH_LOCALIZATION="enabled"

# Performance & Optimization
BRAIN_VISUALIZATION_OPTIMIZATION="enabled"
CDN_OPTIMIZATION="disabled"
REGIONAL_CDN_EUROPE="disabled"

# Security & Encryption (generate secure keys for production)
DATABASE_ENCRYPTION_KEY=""
MEDICAL_DATA_ENCRYPTION_KEY=""
AUDIT_LOG_ENCRYPTION_KEY=""

# Third-party Services (optional for development)
OPENAI_API_KEY=""
STRIPE_PUBLISHABLE_KEY=""
STRIPE_SECRET_KEY=""
STRIPE_WEBHOOK_SECRET=""

# Monitoring & Analytics
SENTRY_DSN=""
VERCEL_ANALYTICS="disabled"
ERROR_TRACKING="enabled"

# Development Tools
ENABLE_DEBUG_LOGS="true"
ENABLE_MOCK_DATA="true"
ENABLE_PLAYGROUND="true"

# Deployment Configuration
DEPLOYMENT_REGION="fra1"
BUILD_ID="development"

# Feature Flags
FEATURE_BRAIN_3D="enabled"
FEATURE_SUPPLEMENT_TRACKING="enabled"
FEATURE_GAMIFICATION="enabled"
FEATURE_ADVANCED_ANALYTICS="disabled"

# Client-side Public Variables
NEXT_PUBLIC_MEDICAL_DISCLAIMER="enabled"
NEXT_PUBLIC_GDPR_COMPLIANCE="strict"
NEXT_PUBLIC_DEFAULT_LOCALE="pl"
NEXT_PUBLIC_APP_VERSION="1.0.0-dev"
NEXT_PUBLIC_SUPPORT_EMAIL=""

NEXT_PUBLIC_FEATURE_BRAIN_3D="enabled"
NEXT_PUBLIC_FEATURE_SUPPLEMENT_TRACKING="enabled"
NEXT_PUBLIC_FEATURE_GAMIFICATION="enabled"

NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=""
NEXT_PUBLIC_SENTRY_DSN=""

NEXT_PUBLIC_ENABLE_DEBUG="true"
NEXT_PUBLIC_ENABLE_MOCK_DATA="true"