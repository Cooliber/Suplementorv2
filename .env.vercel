# Vercel Environment Variables Template
# Copy this to your Vercel project settings

# Next.js Configuration
NODE_ENV=production
VERCEL_ENV=production
NEXT_TELEMETRY_DISABLED=1

# Medical App Configuration
MEDICAL_DATA_PROTECTION=enabled
GDPR_COMPLIANCE_MODE=strict
POLISH_LOCALIZATION=enabled
BRAIN_VISUALIZATION_OPTIMIZATION=enabled
CDN_OPTIMIZATION=enabled
REGIONAL_CDN_EUROPE=enabled

# Performance & Monitoring
ERROR_TRACKING=enabled
VERCEL_ANALYTICS=enabled

# Feature Flags
FEATURE_BRAIN_3D=enabled
FEATURE_SUPPLEMENT_TRACKING=enabled
FEATURE_GAMIFICATION=enabled
FEATURE_ADVANCED_ANALYTICS=disabled

# Public Environment Variables
NEXT_PUBLIC_MEDICAL_DISCLAIMER=enabled
NEXT_PUBLIC_GDPR_COMPLIANCE=strict
NEXT_PUBLIC_DEFAULT_LOCALE=pl
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENABLE_DEBUG=false
NEXT_PUBLIC_ENABLE_MOCK_DATA=false

# Deployment Configuration
DEPLOYMENT_REGION=fra1

# Database (Optional - using hardcoded data for public access)
# MONGODB_URI=your_mongodb_atlas_uri_here
# DATABASE_ENCRYPTION_KEY=your_encryption_key_here
# MEDICAL_DATA_ENCRYPTION_KEY=your_medical_encryption_key_here
# AUDIT_LOG_ENCRYPTION_KEY=your_audit_encryption_key_here

# Authentication (Disabled for public access)
# NEXTAUTH_SECRET=your_nextauth_secret_here
# NEXTAUTH_URL=your_nextauth_url_here

# External Services (Optional)
# OPENAI_API_KEY=your_openai_key_here
# STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
# STRIPE_SECRET_KEY=your_stripe_secret_key_here
# STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here
# SENTRY_DSN=your_sentry_dsn_here
